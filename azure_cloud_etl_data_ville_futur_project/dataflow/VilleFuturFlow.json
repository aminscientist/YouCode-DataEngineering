{
	"name": "VilleFuturFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "DimDate"
				},
				{
					"name": "DimRegion"
				},
				{
					"name": "source3"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DimDateCleaned",
						"type": "DatasetReference"
					},
					"name": "DimDateCleaned"
				},
				{
					"dataset": {
						"referenceName": "DimRegionCleaned",
						"type": "DatasetReference"
					},
					"name": "DimRegionCleaned"
				},
				{
					"dataset": {
						"referenceName": "FactWeatherMetricsCleaned",
						"type": "DatasetReference"
					},
					"name": "FactWeatherMetrics"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          DateID as integer,",
				"          Date as date 'yyyy-MM-dd'",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> DimDate",
				"source(output(",
				"          RegionID as integer,",
				"          RegionName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> DimRegion",
				"source(output(",
				"          DateID as integer,",
				"          RegionID as integer,",
				"          AverageTemperature as integer,",
				"          Humidity as integer,",
				"          WindSpeed as integer,",
				"          Rainfall as integer,",
				"          SunshineHours as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> source3",
				"DimDate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     staged: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> DimDateCleaned",
				"DimRegion sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     staged: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> DimRegionCleaned",
				"source3 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     staged: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> FactWeatherMetrics"
			]
		}
	}
}